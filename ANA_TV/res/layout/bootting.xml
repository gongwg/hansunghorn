<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >
        <Button
            android:id="@+id/statisticsGraph"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="20px"
            android:layout_marginTop="220px"
            android:layout_marginRight="20px"
            android:layout_marginBottom="40px"
            android:text="현재 설문 통계 그래프" />
        <Button
            android:id="@+id/QuestionnaireInitialSettinig"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="20px"
            android:layout_marginRight="20px"
            android:layout_marginBottom="40px"
            android:text="새 설문지  설정" />
        <Button
            android:id="@+id/QuestionnaireSettingImfomation"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="20px"
            android:layout_marginRight="20px"
            android:layout_marginBottom="40px"
            android:text="현 설문지 세팅 내용 보기" />
    </LinearLayout>
    

</LinearLayout>